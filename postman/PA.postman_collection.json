		{
			"name": "loginDriver",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const json = pm.response.json();\r",
							"  const token = json?.data?.token;\r",
							"  if (token) {\r",
							"    pm.environment.set('token', token);\r",
							"    console.log('Token salvato in environment:', token);\r",
							"  } else {\r",
							"    console.warn('Token non trovato in json.data.token');\r",
							"  }"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
